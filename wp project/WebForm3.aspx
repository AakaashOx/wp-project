<body style="height: 913px; width: 1340px;">
    <form id="form1" runat="server">

        <div  style="margin-left: auto; margin-right: auto; text-align: center;" class="auto-style48">
            <asp:Label ID="Label1" runat="server" CssClass="auto-style2" Text="PASSENGER DETAILS" Font-Bold="True" Font-Italic="False" Font-Names="Trebuchet MS" ForeColor="White"></asp:Label>
        </div>

        <div class="auto-style1">
            
            <table class="auto-style3" style="border: thick groove #000000; font-family: Dubai; font-size: 24px">
                <tr>
                    <td class="auto-style33" style="border: thin groove #000000">

                        <asp:Label ID="Label2" runat="server" CssClass="auto-style4" Text="Name :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style28" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox1" runat="server" CssClass="auto-style17" Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black" ></asp:TextBox>
                        <asp:RequiredFieldValidator ID="RequiredFieldValidator1" runat="server" ControlToValidate="TextBox1" CssClass="auto-style34" ErrorMessage="RequiredFieldValidator">Please enter your name</asp:RequiredFieldValidator>

                    </td>
                </tr>
                <tr>
                    <td class="auto-style50" style="border: thin groove #000000">

                        <asp:Label ID="Label3" runat="server" CssClass="auto-style5" Text="Email :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style51" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox2" runat="server" CssClass="auto-style18" Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RegularExpressionValidator ID="RegularExpressionValidator1" runat="server" ControlToValidate="TextBox2" CssClass="auto-style35" ErrorMessage="RegularExpressionValidator" ValidationExpression="\w+([-+.']\w+)*@\w+([-.]\w+)*\.\w+([-.]\w+)*">Please enter valid email</asp:RegularExpressionValidator>

                    </td>
                </tr>
               
                <tr>
                    <td class="auto-style33" style="border: thin groove #000000">

                        <asp:Label ID="Label7" runat="server" CssClass="auto-style9" Text="Date of birth :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style28" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox6" runat="server" CssClass="auto-style22" TextMode="Date" Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RequiredFieldValidator ID="RequiredFieldValidator3" runat="server" ControlToValidate="TextBox6" CssClass="auto-style39" ErrorMessage="RequiredFieldValidator">Please enter your DOB</asp:RequiredFieldValidator>

                        <asp:RequiredFieldValidator ID="RequiredFieldValidator5" runat="server" ControlToValidate="RadioButtonList1" CssClass="auto-style49" ErrorMessage="RequiredFieldValidator">Please select an option</asp:RequiredFieldValidator>

                    </td>
                </tr>
                <tr>
                    <td class="auto-style23" style="border: thin groove #000000">

                        &nbsp;</td>
                    <td class="auto-style29" style="border: thin groove #000000">

                        <asp:RadioButtonList ID="RadioButtonList1" runat="server" CssClass="auto-style24" ValidateRequestMode="Enabled">
                            <asp:ListItem>Male</asp:ListItem>
                            <asp:ListItem>Female</asp:ListItem>
                        </asp:RadioButtonList>

                    </td>
                </tr>
                <tr>
                    <td class="auto-style33" style="border: thin groove #000000">

                        <asp:Label ID="Label9" runat="server" CssClass="auto-style12" Text="Gender :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style28" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox7" runat="server" CssClass="auto-style25"  Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RegularExpressionValidator ID="RegularExpressionValidator3" runat="server" ControlToValidate="TextBox7" CssClass="auto-style40" ErrorMessage="RegularExpressionValidator" ValidationExpression="^[6-9][0-9]{9}$">Please enter valid phone no.</asp:RegularExpressionValidator>

                    </td>
                </tr>
                <tr>
                    <td class="auto-style16" style="border: thin groove #000000">

                        <asp:Label ID="Label10" runat="server" CssClass="auto-style13" Text="Aadhar card number :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                        <asp:Label ID="Label8" runat="server" CssClass="auto-style10" Text="Phone Number :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style30" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox8" runat="server" CssClass="auto-style26"  Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RequiredFieldValidator ID="RequiredFieldValidator4" runat="server" ControlToValidate="TextBox8" CssClass="auto-style41" ErrorMessage="RequiredFieldValidator">Please enter your address</asp:RequiredFieldValidator>

                    </td>
                </tr>
                <tr>
                    <td class="auto-style15" style="border: thin groove #000000">

                        <asp:Label ID="Label11" runat="server" CssClass="auto-style14" Text="Address :" Font-Bold="False" Font-Names="Dubai"></asp:Label>

                    </td>
                    <td class="auto-style31" style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox9" runat="server" CssClass="auto-style27"  Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RegularExpressionValidator ID="RegularExpressionValidator4" runat="server" ControlToValidate="TextBox9" CssClass="auto-style42" ErrorMessage="RegularExpressionValidator" ValidationExpression="^\d{4}\s\d{4}\s\d{4}$">Please enter valid Aadhar No.</asp:RegularExpressionValidator>

                    </td>
                </tr>
                <tr>
                <td class="auto-style33" style="border: thin groove #000000">
                    <asp:Label ID="Label12" runat="server" CssClass="auto-style46" Font-Bold="False" Font-Names="Dubai">Passport number :</asp:Label>
                </td>
                    <td style="border: thin groove #000000">

                        <asp:TextBox ID="TextBox10" runat="server" CssClass="auto-style44"  Font-Bold="True" Font-Names="Century Gothic" Font-Size="Medium" Font-Strikeout="False" Font-Underline="False" ForeColor="Black"></asp:TextBox>
                        <asp:RegularExpressionValidator ID="RegularExpressionValidator5" runat="server" ControlToValidate="TextBox10" CssClass="auto-style45" ErrorMessage="RegularExpressionValidator" ValidationExpression="[A-Z]{1}[0-9]{7}">Please enter valid passport</asp:RegularExpressionValidator>

                    </td>
                </tr>

            </table>
            <asp:Button ID="Button1" runat="server" CssClass="auto-style32" Text="BOOK" OnClick="Button1_Click" Font-Bold="True" Font-Names="Arial Black" Font-Size="Medium" ForeColor="White" />
            <asp:Label ID="Label13" runat="server" CssClass="auto-style47" Font-Bold="True" Font-Names="Century Gothic" Font-Size="Large" Font-Strikeout="False" ForeColor="Black"></asp:Label>
